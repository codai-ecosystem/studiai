    "jsx-a11y/aria-describedby": "off",
    "jsx-a11y/aria-level": "off",
    "jsx-a11y/aria-live": "off",
    "jsx-a11y/aria-relevant": "off",
    "jsx-a11y/aria-sort": "off",
    "jsx-a11y/aria-valuemax": "off",
    "jsx-a11y/aria-valuemin": "off",
    "jsx-a11y/aria-valuenow": "off",
